<?php 
print "Hello!World!<br>"; 
function isPrime($num) {
   if ($num <= 1) {
       return false;
   }
   if ($num <= 3) {
       return true;
   }
   if ($num % 2 == 0 || $num % 3 == 0) {
       return false;
   }
   for ($i = 5; $i * $i <= $num; $i += 6) {
       if ($num % $i == 0 || $num % ($i + 2) == 0) {
           return false;
       }
   }
   return true;
}
for( $i = 1; $i < $_GET['name']; $i++) 
{
   if(isPrime($i))
   {
      print $i.",";
   }
}
